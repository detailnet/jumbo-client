{
    "name": "detailnet/jumbo-client",
    "description": "API Client for Jumbo Web Services",
    "type": "library",
    "keywords": [
        "jumbo",
        "detailnet",
        "api",
        "client"
    ],
    "homepage": "https://github.com/detailnet/jumbo-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Curzio Della Santa",
            "email": "curzio.della.santa@detailnet.ch"
        },
        {
            "name": "Christian Schmid",
            "email": "christian.schmid@detailnet.ch"
        },
        {
            "name": "Ivan Wolf",
            "email": "ivan.wolf@detailnet.ch"
        },
        {
            "name": "Kevin JÃ¼ni",
            "email": "kevin.jueni@detailnet.ch"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/guzzle-services": "^1.1",
        "mtdowling/jmespath.php": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "squizlabs/php_codesniffer" : "^2.2",
        "phpmd/phpmd": "^2.2",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Jumbo\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "JumboTest\\Client\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml",
            ".coveralls.yml",
            ".travis.yml",
            "build.xml",
            "build.properties",
            "composer.phar"
        ]
    }
}
